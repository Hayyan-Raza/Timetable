            "jspdf": "^3.0.4",
            "lucide-react": "^0.487.0",
            "motion": "*",
            "next-themes": "^0.4.6",
            "react": "^18.3.1",
            "react-day-picker": "^8.10.1",
            "react-dom": "^18.3.1",
            "react-hook-form": "^7.55.0",
            "react-resizable-panels": "^2.1.7",
            "recharts": "^2.15.2",
            "sonner": "^2.0.3",
            "tailwind-merge": "*",
            "three": "0.160.0",
            "vaul": "^1.1.2",
            "zustand": "^5.0.9"
      },
      "devDependencies": {
            "@types/node": "^20.10.0",
            "@types/react": "^19.2.7",
            "@types/react-dom": "^19.2.3",
            "@vitejs/plugin-react-swc": "^3.10.2",
            "concurrently": "^9.2.1",
            "electron": "^39.2.4",
            "electron-builder": "^26.0.12",
            "vite": "6.3.5",
            "wait-on": "^9.0.3"
      },
      "scripts": {
            "dev": "vite",
            "build": "vite build",
            "build:backend": "cd backend && python -m PyInstaller app.spec --clean --distpath dist",
            "build:frontend": "vite build",
            "build:full": "npm run build:backend && npm run build:frontend",
            "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
            "electron:build": "npm run build:full && electron-builder",
            "electron:build:win64": "npm run build:full && electron-builder --win --x64",
            "electron:build:portable": "npm run build:full && electron-builder --win portable",
            "package": "npm run build:full && electron-builder --win portable"
      },
      "build": {
            "appId": "com.university.timetable",
            "productName": "University Timetable Dashboard",
            "directories": {
                  "output": "release"
            },
            "files": [
                  "dist/**/*",
                  "electron/**/*",
                  "package.json",
                  "assets/**/*"
            ],
            "extraResources": [
                  {
                        "from": "backend/dist/app.exe",
                        "to": "backend/app.exe"
                  }
            ],
            "win": {
                  "icon": "assets/icon.png",
                  "target": [
                        {
                              "target": "portable",
                              "arch": [
                                    "x64"
                              ]
                        }
                  ]
            },
            "portable": {
                  "artifactName": "${productName}-${version}-Portable.${ext}"
            }
      }
}